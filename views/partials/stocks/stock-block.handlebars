<tr>
	<td>{{this.name}}</td>
	<td class="currentPrice" id={{this.id}}> {{this.price}}</td>
	<td>{{this.symbol}}</td>
	<td>{{changesPercentage}}</td>
	<td>
		<input class="qtyOwned" type="text" placeholder="Value">
		<a class="button buyBtn" id="{{this.id}}" data-open="purchaseModal" href="#">Buy Now</a></td>
</tr>
	
