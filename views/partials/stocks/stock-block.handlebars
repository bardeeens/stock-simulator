 <div class="grid-x opacity-card">
       <div class="cell small-11 large-8 my-card my-stock">
        <div class="grid-x">
            <div class="cell large-12 small-12">{{this.name}}<br>{{this.symbol}}</div>

            <div class="cell large-4 small-4 currentPrice" id={{this.id}}>{{this.price}}</div>
            {{!-- <div class="cell large-2 small-2"></div> --}}
        	<input id="buyQty{{this.id}}" class="cell large-4 small-4 cash" type="text" placeholder="Qty">
			<button id="{{this.id}}" class="buyBtn button very-special"  data-open="purchaseModal" href="#" class="button postfix buyBtn">Buy Now</button>
        </div>
       </div>






