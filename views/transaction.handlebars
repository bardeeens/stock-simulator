<div data-sticky-container>
    <div data-sticky data-options="marginTop:0;">
  
      <div class="top-bar" id="example-menu">
        <ul class="vertical medium-horizontal dropdown menu" data-responsive-menu="accordion medium-dropdown">
          <li class="menu-text">Transactions</li>
          <li><a id="portfolioBtn" class="navBtns" >My Portfolio</a></li>
          <li><a class="navBtns" href="../market">Marketplace</a></li>
          <li><a class="navBtns" href="/">Sign Out</a></li>
        </ul>
      </div>
  </div>
</div>
<br>
<div class="holy-grail-middle">
    <div class="holy-grail-middle">
        <table class="pricing-table hover">
            <thead>
                <h3> My Transaction History</h3>
                <tr>
                    <th class="name" width="100">Name</th>
                    <th class="symbol" width="75">Ticker</th>
                    <th class="price" width="75">Purchase Price</th>
                    <th class="sharesPurchased" width="100">Shares</th>
                    <th class="sharesPurchased" width="100">Price</th>
                    <th class="value" width="75">Total Value</th>
                </tr>
            </thead>
                {{#each transactions}}
                <tr>
                    <td>{{Stock.name}}</td>
                    <td>{{Stock.symbol}}</td>
                    <td>{{price}}</td>
                    <td>{{qty}}</td>
                    <td>{{price}}</td>
                    <td>{{totalValue}}</td>
                </tr>
                {{/each}}

            </tbody>
        </table>

    </div>